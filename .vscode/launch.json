    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch google auth",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.js",
            "env": {
                "AUTH": "google"
            }
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch ping auth",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.js",
            "env": {
                "AUTH": "ping"
            }
        }
    ]
}